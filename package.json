{
  "name": "jimeng-web-mcp",
  "version": "2.0.2",
  "description": "MCP服务器项目，直接访问即梦AI Web端进行图像和视频生成（仅供学习研究使用）",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./server": {
      "types": "./lib/server.d.ts",
      "import": "./lib/server.js",
      "require": "./lib/server.cjs"
    }
  },
  "bin": {
    "jimeng-web-mcp": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "image-generation",
    "video-generation",
    "jimeng",
    "claude",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LupinLin1/jimeng-web-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/LupinLin1/jimeng-web-mcp/issues"
  },
  "homepage": "https://github.com/LupinLin1/jimeng-web-mcp#readme",
  "author": "rick <1196363729@qq.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node -r dotenv/config lib/index.js",
    "start:dev": "nodemon -r dotenv/config lib/index.js",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:mcp": "npx @modelcontextprotocol/inspector node -r dotenv/config ./lib/index.js",
    "start:api": "dotenv -e .env --  tsx ./script/start",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.3",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.1",
    "tsup": "^8.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/image-size": "^0.7.0",
    "axios": "^1.9.0",
    "crc32": "^0.2.2",
    "dotenv": "^16.5.0",
    "image-size": "^2.0.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  }
}
